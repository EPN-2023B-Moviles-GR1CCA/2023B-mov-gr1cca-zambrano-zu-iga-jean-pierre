<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_editargenero"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Editar_Genero">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Por favor realice los cambios correspondientes "
        android:textSize="17sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.083" />

    <EditText
        android:id="@+id/input_nombre_genero2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <EditText
        android:id="@+id/input_descripcion_genero2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="@+id/input_nombre_genero2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/input_nombre_genero2"
        app:layout_constraintTop_toBottomOf="@+id/input_nombre_genero2" />

    <EditText
        android:id="@+id/input_cantidad_genero2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="@+id/input_descripcion_genero2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/input_descripcion_genero2"
        app:layout_constraintTop_toBottomOf="@+id/input_descripcion_genero2" />

    <EditText
        android:id="@+id/input_restriccion_edad_genero2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:ems="10"
        android:inputType="text"
        android:text="SI / NO"
        app:layout_constraintEnd_toEndOf="@+id/input_cantidad_genero2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/input_cantidad_genero2"
        app:layout_constraintTop_toBottomOf="@+id/input_cantidad_genero2" />

    <EditText
        android:id="@+id/input_fecha_ingreso_genero2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="text"
        android:text="yyyy-MM-dd"
        app:layout_constraintEnd_toEndOf="@+id/input_restriccion_edad_genero2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/input_restriccion_edad_genero2"
        app:layout_constraintTop_toBottomOf="@+id/input_restriccion_edad_genero2" />

    <Button
        android:id="@+id/btn_editar_genero"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="Editar informacion"
        app:layout_constraintEnd_toEndOf="@+id/input_fecha_ingreso_genero2"
        app:layout_constraintHorizontal_bias="0.309"
        app:layout_constraintStart_toStartOf="@+id/input_fecha_ingreso_genero2"
        app:layout_constraintTop_toBottomOf="@+id/input_fecha_ingreso_genero2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Nombre del gnero"
        app:layout_constraintBottom_toTopOf="@+id/input_nombre_genero2"
        app:layout_constraintEnd_toEndOf="@+id/input_nombre_genero2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/input_nombre_genero2" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Descripcion del genero"
        app:layout_constraintBottom_toTopOf="@+id/input_descripcion_genero2"
        app:layout_constraintEnd_toEndOf="@+id/input_descripcion_genero2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/input_descripcion_genero2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Copias disponibles"
        app:layout_constraintEnd_toEndOf="@+id/input_cantidad_genero2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/input_cantidad_genero2"
        tools:layout_editor_absoluteY="290dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Mayor de edad"
        app:layout_constraintBottom_toTopOf="@+id/input_restriccion_edad_genero2"
        app:layout_constraintEnd_toEndOf="@+id/input_restriccion_edad_genero2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/input_restriccion_edad_genero2" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:text="Fecha de ingreso"
        app:layout_constraintBottom_toTopOf="@+id/input_fecha_ingreso_genero2"
        app:layout_constraintEnd_toEndOf="@+id/input_fecha_ingreso_genero2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/input_fecha_ingreso_genero2" />
</androidx.constraintlayout.widget.ConstraintLayout>